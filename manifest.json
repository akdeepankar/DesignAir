
  {
    "manifest_version": 3,
    "name": "Color Palette Capture",
    "version": "1.0",
    "permissions": [
      "tabs",
      "activeTab",
      "scripting"
    ],
    "icons": {
      "16": "images/icon16.png",
      "48": "images/icon48.png",
      "128": "images/icon128.png"
    },
    "action": {
      "default_popup": "popup.html"
    },
    "background": {
      "service_worker": "background.js"
    },
    "web_accessible_resources": [
    {
      "resources": ["*"],
      "matches": ["<all_urls>"]
    }
  ]
  }
  